{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "**/*.js": {
            "maxDuration": 5
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}